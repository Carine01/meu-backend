{
  "name": "elevare-atendimento-backend",
  "version": "1.0.0",
  "description": "Backend para Elevare Atendimento",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "start:dev": "tsc && nodemon dist/main.js",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:cov": "jest --coverage --config jest.config.js",
    "test:e2e": "jest --config test/jest-e2e.json",
    "clean:test-data": "ts-node src/scripts/clean-test-data.ts",
    "seed": "ts-node src/scripts/seed.ts",
    "seed:admin": "ts-node src/scripts/seed-admin.ts",
    "setup": "bash scripts/setup.sh",
    "setup:windows": "scripts\\setup.bat",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d ormconfig.ts",
    "migration:run": "npm run typeorm -- migration:run -d ormconfig.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ormconfig.ts"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^10.0.0",
    "@whiskeysockets/baileys": "^7.0.0-rc.9",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "firebase-admin": "^13.6.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "joi": "^17.9.2",
    "nestjs-pino": "^4.4.1",
    "node-cron": "^4.2.1",
    "nodemon": "^3.1.11",
    "p-retry": "^7.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/passport-jwt": "^4.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  }
}
